@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix fixm:  <http://www.aisa-project.eu/vocabulary/fixm_3-0-1_sesar#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

fixm:ClearedFlightInformation
        a            rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:class     fixm:DirectRouting ;
                       sh:maxCount  1 ;
                       sh:path      fixm:directRouting
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:VerticalRate ;
                       sh:path      fixm:rateOfClimbDescend
                     ] ;
        sh:property  [ sh:class     fixm:LateralOfftrack ;
                       sh:maxCount  1 ;
                       sh:path      fixm:offtrackClearance
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:Direction ;
                       sh:path      fixm:heading
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:AirspeedChoice ;
                       sh:path      fixm:clearedSpeed
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:AltitudeChoice ;
                       sh:path      fixm:clearedFlightLevel
                     ] .

fixm:ConstrainedAirspace
        a            sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:AirTrafficType  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "OAT" "GAT" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:Direction  a    sh:NodeShape ;
        sh:and       ( fixm:Angle ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:DirectionReference ;
                       sh:path      fixm:ref
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:EfplFlightRules  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "IFR" "IFR_THEN_VFR" "VFR" "VFR_THEN_IFR" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:AltitudeMeasure  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "FEET" "METRES" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      fixm:uom
                     ] .

fixm:IcaoAerodromeReference
        a                rdfs:Class , sh:NodeShape ;
        rdfs:subClassOf  fixm:AerodromeReference ;
        sh:and           ( fixm:AerodromeReference ) ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:AerodromeIcaoCode ;
                           sh:path      fixm:code
                         ] .

fixm:SpeedSchedule  a  rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:AirspeedInIasOrMach ;
                       sh:path      fixm:subsequentSpeed
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:AirspeedInIasOrMach ;
                       sh:path      fixm:initialSpeed
                     ] .

fixm:SlotAssignment  a  sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:DistanceMeasure  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "NAUTICAL_MILES" "MILES" "KILOMETERS" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      fixm:uom
                     ] .

fixm:MarinePollutantIndicator
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "MARINE_POLLUTANT" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:AircraftDangerousGoodsLimitation
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "PASSENGER_AND_CARGO_AIRCRAFT" "CARGO_AIRCRAFT_ONLY" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:Count  a        sh:NodeShape ;
        sh:property  [ sh:datatype      xsd:integer ;
                       sh:maxCount      1 ;
                       sh:minInclusive  0 ;
                       sh:path          rdf:value
                     ] .

fixm:Pressure  a     sh:NodeShape ;
        sh:and       ( fixm:PressureMeasure ) ;
        sh:and       ( fixm:UnitOfMeasure ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:Groundspeed  a  sh:NodeShape ;
        sh:and       ( fixm:GroundspeedMeasure ) ;
        sh:and       ( fixm:UnitOfMeasure ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:ProvenanceSource
        a            sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:GloballyUniqueFlightIdentifier
        a            sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      [ a            sh:NodeShape ;
                                      sh:property  [ sh:datatype  xsd:string ;
                                                     sh:path      rdf:value
                                                   ]
                                    ] ;
                       sh:path      fixm:codeSpace
                     ] ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^[0-9|aA-fF]{8}-[0-9|aA-fF]{4}-4[0-9|aA-fF]{3}-[89aAbB]{1}[0-9|aA-fF]{3}-[0-9|aA-fF]{12}$"
                     ] .

fixm:OtherInformation
        a            rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:Distance ;
                       sh:path      fixm:runwayVisualRange
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      [ a            sh:NodeShape ;
                                      sh:property  [ sh:datatype  xsd:integer ;
                                                     sh:path      rdf:value
                                                   ]
                                    ] ;
                       sh:path      fixm:replacementFlightPlanIndicator
                     ] .

fixm:RadioCommunicationFailure
        a                rdfs:Class , sh:NodeShape ;
        rdfs:subClassOf  fixm:Feature ;
        sh:and           ( fixm:Feature ) ;
        sh:property      [ sh:class     fixm:LastContact ;
                           sh:maxCount  1 ;
                           sh:path      fixm:contact
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:FreeText ;
                           sh:path      fixm:remainingComCapability
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:FreeText ;
                           sh:path      fixm:radioFailureRemarks
                         ] .

fixm:WindDirection  a  sh:NodeShape ;
        sh:and       ( fixm:Angle ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:MeteorologicalData
        a            rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:Windspeed ;
                       sh:path      fixm:windSpeed
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:WindDirection ;
                       sh:path      fixm:windDirection
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:Temperature ;
                       sh:path      fixm:temperature
                     ] .

fixm:AtnLogonParameters
        a            sh:NodeShape ;
        sh:and       ( fixm:Parameters ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:CpdlcStartRequestIndicator
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "SEND_CPDLC_START_REQUEST" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:FrequencyUsage  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "VOICE" "CPDLC" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:Fans1ALogonParameters
        a            sh:NodeShape ;
        sh:and       ( fixm:Parameters ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:PackingGroup  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "I" "II" "III" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:SupplementalData
        a            rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:class     fixm:Person ;
                       sh:maxCount  1 ;
                       sh:path      fixm:pilotInCommand
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:Count ;
                       sh:path      fixm:personsOnBoard
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:Duration ;
                       sh:path      fixm:fuelEndurance
                     ] .

fixm:TextCountryCode  a  sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^[A-Z]{2}$"
                     ] .

fixm:VolumeMeasure  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "LITRES" "GALLONS" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      fixm:uom
                     ] .

fixm:EnRoute  a          sh:NodeShape , rdfs:Class ;
        rdfs:subClassOf  fixm:Feature ;
        sh:and           ( fixm:Feature ) ;
        sh:property      [ sh:class     fixm:Pointout ;
                           sh:maxCount  1 ;
                           sh:path      fixm:pointout
                         ] ;
        sh:property      [ sh:class  fixm:UnitBoundary ;
                           sh:path   fixm:boundaryCrossings
                         ] ;
        sh:property      [ sh:class     fixm:ClearedFlightInformation ;
                           sh:maxCount  1 ;
                           sh:path      fixm:cleared
                         ] ;
        sh:property      [ sh:class  fixm:AerodromeReference ;
                           sh:path   fixm:alternateAerodrome
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:FleetPriority ;
                           sh:path      fixm:fleetPrioritization
                         ] ;
        sh:property      [ sh:class     fixm:CpdlcConnection ;
                           sh:maxCount  1 ;
                           sh:path      fixm:cpdlcConnection
                         ] ;
        sh:property      [ sh:class     fixm:BeaconCodeAssignment ;
                           sh:maxCount  1 ;
                           sh:path      fixm:beaconCodeAssignment
                         ] ;
        sh:property      [ sh:node  fixm:ControlElement ;
                           sh:path  fixm:controlElement
                         ] ;
        sh:property      [ sh:class     fixm:AircraftPosition ;
                           sh:maxCount  1 ;
                           sh:path      fixm:position
                         ] .

fixm:TextCity  a     sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:LifeJacketCode  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "FLUORESCEIN" "VERY_HIGH_FREQUENCY" "LIGHTS" "ULTRA_HIGH_FREQUENCY" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:AirspeedChoice  a  sh:NodeShape ;
        sh:xone  ( [ sh:node  fixm:Speed ]
                   [ sh:class  fixm:AirspeedRange ]
                 ) .

fixm:AtcUnitName  a  sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^([A-Z]{4})|([A-Za-z0-9]{1,})$"
                     ] .

fixm:UnitBoundaryIndicator
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "PAST" "CURRENT" "FUTURE" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:OperatorCategory
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "AIR_CARRIER" "FREIGHT_CARGO_CARRIER" "GENERAL_AVIATION" "MILITARY" "AIR_TAXI" "OTHER" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:CompatibilityGroup
        a            sh:NodeShape ;
        sh:and       ( fixm:FreeText ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^[A-H|K-L|N|S]$"
                     ] .

fixm:LastContact  a      rdfs:Class , sh:NodeShape ;
        rdfs:subClassOf  fixm:Feature ;
        sh:and           ( fixm:Feature ) ;
        sh:property      [ sh:class     fixm:LastPositionReport ;
                           sh:maxCount  1 ;
                           sh:path      fixm:position
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:AtcUnitName ;
                           sh:path      fixm:lastContactUnit
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:Time ;
                           sh:path      fixm:lastContactTime
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:RadioFrequency ;
                           sh:path      fixm:contactFrequency
                         ] .

fixm:PersonOrOrganization
        a        sh:NodeShape ;
        sh:xone  ( [ sh:class  fixm:Organization ]
                   [ sh:class  fixm:Person ]
                 ) .

fixm:Time  a         sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:dateTime ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:VerticalRate  a  sh:NodeShape ;
        sh:and       ( fixm:UnitOfMeasure ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:minCount  1 ;
                       sh:node      fixm:VerticalRateMeasure ;
                       sh:path      fixm:uom
                     ] ;
        sh:property  [ sh:maxCount      1 ;
                       sh:maxInclusive  3000 ;
                       sh:minInclusive  -3000 ;
                       sh:path          rdf:value
                     ] .

fixm:RadioFrequency  a  sh:NodeShape ;
        sh:and       ( fixm:UnitOfMeasure ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:minCount  1 ;
                       sh:node      fixm:FrequencyMeasure ;
                       sh:path      fixm:uom
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^[0-9]{3}\\.[0-9]{2}$"
                     ] .

fixm:WindSpeedMeasure
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "MILES_PER_HOUR" "KILOMETRES_PER_HOUR" "KNOTS" "METERS_PER_SECOND" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      fixm:uom
                     ] .

fixm:AftnAddress  a  sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:NonStandardCoordinationReason
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "LATE_ACTIVATION" "LATERAL_DEVIATION" "LATE_REVISION" "NON_STANDARD_TFL" "NON_STANDARD_EQUIPMENT" "NON_STANDARD_SSR_CODE" "TRANSITION_POINT" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:ProvenanceSystem
        a            sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:FrequencyMeasure
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "MEGAHERTZ" "KILOHERTZ" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      fixm:uom
                     ] .

fixm:IcaoAircraftIdentifier
        a            sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^[A-Z0-9]{2,4}$"
                     ] .

fixm:RouteConstraintOrPreference
        a            rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:ConstraintOrPreferenceCategory ;
                       sh:path      fixm:constraintType
                     ] .

fixm:LateralOfftrack  a  rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:OfftrackReason ;
                       sh:path      fixm:offtrackReason
                     ] ;
        sh:property  [ sh:class     fixm:OfftrackDistance ;
                       sh:maxCount  2 ;
                       sh:path      fixm:offtrackDistance
                     ] .

fixm:FissileExcepted  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "EXCEPTED" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:ActualSpeed  a  rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:Groundspeed ;
                       sh:path      fixm:surveillance
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:TrueAirspeedOrMach ;
                       sh:path      fixm:pilotReported
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:Groundspeed ;
                       sh:path      fixm:calculated
                     ] .

fixm:UnitBoundary  a     rdfs:Class , sh:NodeShape ;
        rdfs:subClassOf  fixm:AtcUnitReference ;
        sh:and           ( fixm:AtcUnitReference ) ;
        sh:property      [ sh:class     fixm:Handoff ;
                           sh:maxCount  1 ;
                           sh:path      fixm:handoff
                         ] ;
        sh:property      [ sh:class     fixm:BoundaryCrossing ;
                           sh:maxCount  1 ;
                           sh:path      fixm:boundaryCrossingCoordinated
                         ] ;
        sh:property      [ sh:class     fixm:AtcUnitReference ;
                           sh:maxCount  1 ;
                           sh:path      fixm:upstreamUnit
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:UnitBoundaryIndicator ;
                           sh:path      fixm:unitBoundaryIndicator
                         ] ;
        sh:property      [ sh:class     fixm:BoundaryCrossing ;
                           sh:maxCount  1 ;
                           sh:path      fixm:boundaryCrossingProposed
                         ] ;
        sh:property      [ sh:class     fixm:AtcUnitReference ;
                           sh:maxCount  1 ;
                           sh:path      fixm:downstreamUnit
                         ] .

fixm:ProvenanceCentre
        a            sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:AtcUnitReference
        a            rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:DelegatedUnitIndicator ;
                       sh:path      fixm:delegated
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:FreeText ;
                       sh:path      fixm:sectorIdentifier
                     ] .

fixm:CommunicationCapabilities
        a            rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:node  fixm:CommunicationCode ;
                       sh:path  fixm:communicationCode
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:SelectiveCallingCode ;
                       sh:path      fixm:selectiveCallingCode
                     ] ;
        sh:property  [ sh:node  fixm:DataLinkCode ;
                       sh:path  fixm:dataLinkCode
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:FreeText ;
                       sh:path      fixm:otherDataLinkCapabilities
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:FreeText ;
                       sh:path      fixm:otherCommunicationCapabilities
                     ] .

fixm:ShippingInformation
        a            rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:class     fixm:StructuredPostalAddress ;
                       sh:maxCount  1 ;
                       sh:path      fixm:consignee
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:FreeText ;
                       sh:path      fixm:shipmentAuthorizations
                     ] ;
        sh:property  [ sh:class     fixm:StructuredPostalAddress ;
                       sh:maxCount  1 ;
                       sh:path      fixm:shipper
                     ] ;
        sh:property  [ sh:class     fixm:DeclarationText ;
                       sh:maxCount  1 ;
                       sh:path      fixm:declarationText
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:FreeText ;
                       sh:path      fixm:supplementaryInformation
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:FreeText ;
                       sh:path      fixm:dangerousGoodsScreeningLocation
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:TextCountryName ;
                       sh:path      fixm:originCountry
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:TextCountryName ;
                       sh:path      fixm:destinationCountry
                     ] ;
        sh:property  [ sh:node  fixm:AerodromeIcaoCode ;
                       sh:path  fixm:transferAerodromes
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:FreeText ;
                       sh:path      fixm:subsidiaryHazardClassAndDivision
                     ] ;
        sh:property  [ sh:class     fixm:AerodromeReference ;
                       sh:maxCount  1 ;
                       sh:path      fixm:aerodromeOfLoading
                     ] ;
        sh:property  [ sh:class     fixm:AerodromeReference ;
                       sh:maxCount  1 ;
                       sh:path      fixm:aerodromeOfUnloading
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      fixm:TextCountryName ;
                       sh:path      fixm:departureCountry
                     ] .

fixm:ExpandedRoutePoint
        a                rdfs:Class , sh:NodeShape ;
        rdfs:subClassOf  fixm:AbstractRoutePoint ;
        sh:and           ( fixm:AbstractRoutePoint ) ;
        sh:property      [ sh:class  fixm:RouteConstraintOrPreference ;
                           sh:path   fixm:constraint
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:Time ;
                           sh:path      fixm:estimatedTime
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      fixm:Altitude ;
                           sh:path      fixm:estimatedLevel
                         ] .

fixm:Longitude  a    sh:NodeShape ;
        sh:and       ( fixm:UnitOfMeasure ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:SelectiveCallingCode
        a            sh:NodeShape ;
        sh:and       ( fixm:FreeText ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^[A-HJ-MP-S]{4}$"
                     ] .

fixm:ClearanceLimitIndicator
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "CLEARANCE_LIMIT" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      rdf:value
                     ] .

fixm:WeightMeasure  a  sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "KILOGRAMS" "POUNDS" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      fixm:uom
                     ] .

fixm:TimeSequence  a  rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:class     fixm:MultiTime ;
                       sh:maxCount  1 ;
                       sh:path      fixm:request
                     ] ;
        sh:property  [ sh:class     fixm:MultiTime ;
                       sh:maxCount  1 ;
                       sh:path      fixm:ready
                     ] ;
        sh:property  [ sh:class     fixm:MultiTime ;
                       sh:maxCount  1 ;
                       sh:path      fixm:end
                     ] ;
        sh:property  [ sh:class     fixm:MultiTime ;
                       sh:maxCount  1 ;
                       sh:path      fixm:begin
                     ] ;
        sh:property  [ sh:class     fixm:TargetMultiTime ;
                       sh:maxCount  1 ;
                       sh:path      fixm:approval
                     ] .

fixm:RadioactivityMeasure
        a            sh:NodeShape ;
        sh:property  [ sh:in        [ rdf:rest  ( "GRAMS" "BECQUERELS" )
                                    ] ;
                       sh:minCount  1 ;
                       sh:path      fixm:uom
                     ] .

