@prefix aixm:  <http://www.aisa-project.eu/vocabulary/aixm_5-1-1#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix gml:   <http://www.opengis.net/gml/3.2#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

aixm:AirportHeliportResponsibilityOrganisation
        a                rdfs:Class , sh:NodeShape ;
        rdfs:subClassOf  aixm:PropertiesWithSchedule ;
        sh:and           ( aixm:PropertiesWithSchedule ) ;
        sh:property      [ sh:class     aixm:OrganisationAuthority ;
                           sh:maxCount  1 ;
                           sh:minCount  1 ;
                           sh:path      aixm:theOrganisationAuthority
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      aixm:CodeAuthorityRoleType ;
                           sh:path      aixm:role
                         ] .

aixm:DateMonthDayType
        a            sh:NodeShape ;
        sh:and       ( aixm:DateMonthDayBaseType ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      aixm:NilReasonEnumeration ;
                       sh:path      aixm:nilReason
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] ;
        sh:xone      ( [ sh:property  [ sh:minCount  1 ;
                                        sh:path      rdf:value
                                      ]
                       ]
                       [ sh:property  [ sh:minCount  1 ;
                                        sh:path      aixm:nilReason
                                      ]
                       ]
                     ) .

aixm:ElevatedPoint  a    rdfs:Class , sh:NodeShape ;
        rdfs:subClassOf  aixm:Point ;
        sh:and           ( aixm:Point ) ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      aixm:ValDistanceType ;
                           sh:path      aixm:verticalAccuracy
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      aixm:CodeVerticalDatumType ;
                           sh:path      aixm:verticalDatum
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      aixm:ValDistanceSignedType ;
                           sh:path      aixm:geoidUndulation
                         ] ;
        sh:property      [ sh:maxCount  1 ;
                           sh:node      aixm:ValDistanceVerticalType ;
                           sh:path      aixm:elevation
                         ] .

gml:TimePrimitive  a  sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:in        [ rdf:rest  ( "after" "before" "now" "unknown" )
                                    ] ;
                       sh:maxCount  1 ;
                       sh:path      gml:indeterminatePosition
                     ] ;
        sh:property  [ sh:datatype  xsd:dateTime ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] ;
        sh:xone      ( [ sh:property  [ sh:minCount  1 ;
                                        sh:path      rdf:value
                                      ]
                       ]
                       [ sh:property  [ sh:minCount  1 ;
                                        sh:path      gml:indeterminatePosition
                                      ]
                       ]
                     ) .

aixm:LinguisticNote  a  rdfs:Class , sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      aixm:TextNoteType ;
                       sh:path      aixm:note
