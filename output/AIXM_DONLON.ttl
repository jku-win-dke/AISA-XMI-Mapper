                       ]
                     ) .

aixm:ValFrictionBaseType
        a            sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:decimal ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^0\\.[0-9]{2}$"
                     ] .

aixm:TextPhoneBaseType
        a            sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^(\\+)?[0-9\\s\\-\\(\\)]+$"
                     ] .

