aixm:ValTemperatureBaseType
        a            sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:decimal ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] .

aixm:CodeAircraftEngineNumberType
        a            sh:NodeShape ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      aixm:NilReasonEnumeration ;
                       sh:path      aixm:nilReason
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      aixm:CodeAircraftEngineNumberBaseType ;
                       sh:path      rdf:value
                     ] ;
        sh:xone      ( [ sh:property  [ sh:minCount  1 ;
                                        sh:path      rdf:value
                                      ]
