        sh:in        [ rdf:rest  ( "MON" "TUE" "WED" "THU" "FRI" "SAT" "SUN" "WORK_DAY" "BEF_WORK_DAY" "AFT_WORK_DAY" "HOL" "BEF_HOL" "AFT_HOL" "ANY" "BUSY_FRI" "OTHER" )
                     ] .

aixm:ValWeightType  a  sh:NodeShape ;
        sh:and       ( aixm:ValWeightBaseType ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      aixm:NilReasonEnumeration ;
                       sh:path      aixm:nilReason
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      aixm:UomWeightType ;
                       sh:path      aixm:uom
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] ;
        sh:xone      ( [ sh:property  [ sh:minCount  1 ;
                                        sh:path      rdf:value
                                      ] ;
                         sh:property  [ sh:path  aixm:uom ]
                       ]
                       [ sh:property  [ sh:minCount  1 ;
                                        sh:path      aixm:nilReason
                                      ]
                       ]
                     ) .

aixm:CodeCommunicationModeBaseType
        a            sh:NodeShape ;
        sh:datatype  xsd:string ;
        sh:in        [ rdf:rest  ( "HF" "VHF" "VDL1" "VDL2" "VDL4" "AMSS" "ADS_B" "ADS_B_VDL" "HFDL" "VHF_833" "UHF" "OTHER" )
                     ] .

aixm:TextAddressBaseType
        a            sh:NodeShape ;
        sh:and       ( aixm:Character2Type ) ;
        sh:property  [ sh:maxCount   1 ;
                       sh:maxLength  500 ;
                       sh:minLength  1 ;
                       sh:path       rdf:value
                     ] .

aixm:AlphanumericType
        a            sh:NodeShape ;
        sh:property  [ sh:datatype  xsd:string ;
                       sh:maxCount  1 ;
                       sh:path      rdf:value ;
                       sh:pattern   "^([A-Z]|\\d)*$"
                     ] .

aixm:TextPropertyNameType
        a            sh:NodeShape ;
        sh:and       ( aixm:TextPropertyNameBaseType ) ;
        sh:property  [ sh:maxCount  1 ;
                       sh:node      aixm:NilReasonEnumeration ;
                       sh:path      aixm:nilReason
                     ] ;
        sh:property  [ sh:maxCount  1 ;
                       sh:path      rdf:value
                     ] ;
        sh:xone      ( [ sh:property  [ sh:minCount  1 ;
                                        sh:path      rdf:value
                                      ]
                       ]
                       [ sh:property  [ sh:minCount  1 ;
                                        sh:path      aixm:nilReason
                                      ]
                       ]
                     ) .

