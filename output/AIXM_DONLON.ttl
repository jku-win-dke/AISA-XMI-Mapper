                                      ]
                       ]
                     ) .

aixm:TextPropertyNameBaseType
        a            sh:NodeShape ;
        sh:property  [ sh:datatype   xsd:string ;
                       sh:maxCount   1 ;
                       sh:maxLength  60 ;
                       sh:minLength  1 ;
                       sh:path       rdf:value ;
                       sh:pattern    "^[A-Za-z\\-_]*$"
                     ] .

aixm:CodeMilitaryStatusBaseType
        a            sh:NodeShape ;
        sh:datatype  xsd:string ;
        sh:in        [ rdf:rest  ( "MIL" "CIVIL" "ALL" "OTHER" )
                     ] .
